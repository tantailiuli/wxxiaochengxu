/* topsearch.wxss */
.searchbox{
  display: flex;
  justify-content: center;
  align-items: center;
  width: 100%;
  height: 75rpx;
  background-color: #f5f5f5;
}
.searchbox .search{
  height: 54rpx;
  width: 689rpx;
  background-color: #fff;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  border-radius: 5rpx;
}
.searchbox .search icon{
  margin: 0 0 0 20rpx;
}
.searchbox .search input{
  flex: auto;
  padding-left: 10rpx;
  font-size: 24rpx;
}